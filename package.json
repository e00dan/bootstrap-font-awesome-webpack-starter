{
  "name": "bootstrap-font-awesome-webpack-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack/prod.config.js",
    "dev": "webpack-dev-server --config webpack/dev.config.js --host 0.0.0.0 --disable-host-check --hot",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "prod": "webpack-dev-server --config webpack/prod.config.js --host 0.0.0.0 --disable-host-check",
    "precommit": "lint-staged",
    "formatAll": "find ./src/ -name '*.js*' | xargs prettier --write --config ./.prettierrc --config-precedence file-override"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write --config ./.prettierrc --config-precedence file-override"
    ]
  },
  "author": "Daniel Kmak",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@babel/plugin-proposal-class-properties": "~7.8.3",
    "@babel/plugin-proposal-decorators": "~7.8.3",
    "@babel/plugin-transform-runtime": "~7.9.0",
    "@codechecks/build-size-watcher": "~0.1.0",
    "@codechecks/client": "~0.1.10",
    "autoprefixer": "~9.7.6",
    "babel-eslint": "~10.1.0",
    "babel-loader": "~8.1.0",
    "babel-preset-env": "~1.7.0",
    "browserslist": "~4.12.0",
    "compression-webpack-plugin": "~3.1.0",
    "css-hot-loader": "~1.4.4",
    "css-loader": "~3.5.2",
    "eslint": "~6.8.0",
    "file-loader": "~6.0.0",
    "font-awesome-loader": "~1.0.2",
    "html-loader": "~1.1.0",
    "html-webpack-plugin": "~4.2.0",
    "husky": "~4.2.5",
    "image-webpack-loader": "~6.0.0",
    "lint-staged": "~10.1.7",
    "mini-css-extract-plugin": "~0.9.0",
    "node-sass": "~7.0.0",
    "optimize-css-assets-webpack-plugin": "~5.0.3",
    "postcss-loader": "~3.0.0",
    "prettier": "~2.0.5",
    "resolve-url-loader": "~3.1.1",
    "sass-loader": "~8.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "~4.1.0",
    "webpack": "~4.43.0",
    "webpack-cli": "~3.3.11",
    "webpack-dev-server": "~3.10.3",
    "webpack-merge": "~4.2.2"
  },
  "dependencies": {
    "bootstrap": "~4.4.1",
    "font-awesome": "~4.7.0",
    "jquery": "~3.5.0",
    "popper.js": "~1.16.1",
    "tether": "~1.4.7"
  },
  "browserslist": [
    "defaults",
    "not ie < 11"
  ]
