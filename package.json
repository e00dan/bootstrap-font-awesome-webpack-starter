{
  "name": "bootstrap-font-awesome-webpack-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack/prod.config.js",
    "dev": "webpack-dev-server --config webpack/dev.config.js --host 0.0.0.0 --disable-host-check --hot",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "prod": "webpack-dev-server --config webpack/prod.config.js --host 0.0.0.0 --disable-host-check",
    "precommit": "lint-staged",
    "formatAll": "find ./src/ -name '*.js*' | xargs prettier --write --config ./.prettierrc --config-precedence file-override"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write --config ./.prettierrc --config-precedence file-override",
      "git add"
    ]
  },
  "author": "Daniel Kmak",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "browserslist": "^3.2.8",
    "compression-webpack-plugin": "^1.1.11",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^0.28.11",
    "eslint": "^5.0.0",
    "file-loader": "^1.1.11",
    "font-awesome-loader": "^1.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.2.0",
    "lint-staged": "^7.1.2",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.12.1",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.0.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.0.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "tether": "^1.4.4"
  },
  "browserslist": [
    "defaults",
    "not ie < 11"
  ]
